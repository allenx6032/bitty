Example for the Bitty Engine

Copyright (C) 2020 - 2022 Tony Wang, all rights reserved

Homepage: https://paladin-t.github.io/bitty/
]]

-- Note:
--   a classic boing ball demo.

--[[
Constant.
]]

local RED = Color.new(255, 0, 0)
local WHITE = Color.new(255, 255, 255)
local ROTATE_ANGLE = 0.5 -- How many radians is the ball tilted.

--[[
Variables.
]]

local sfx = nil
local pal = nil
local ball = nil
local whiteStart = 1
local ballX = 5
local parabelX = 0
local ballDir = 1
local parabelDir = 9.25

--[[
Functions.
]]

-- Shows the background.
local function background()
	local canvasWidth, canvasHeight = Canvas.main:size()
	color(Color.new(225, 150, 225))
	for y = 0, canvasHeight - 33, 16 do
		for x = 23, canvasWidth - 26, 16 do
			rect(x, y, x + 17, y + 17)
		end
	end
	for x = 23, canvasWidth - 25, 16 do
		line(x, canvasHeight - 32, (x - canvasWidth * 0.5) / 410 * canvasWidth + canvasWidth * 0.5 + 1, canvasHeight - 1)
	end
	line(15, canvasHeight - 25, canvasWidth - 16, canvasHeight - 25)
	line(3, canvasHeight - 13, canvasWidth - 4, canvasHeight - 13)
	line(0, canvasHeight - 1, canvasWidth, canvasHeight - 1)
end

-- Cycles the palette.
local function cyclePalette()
	whiteStart = whiteStart - ballDir
	if whiteStart > 13 then whiteStart = 2 end
	if whiteStart < 2 then whiteStart = 13 end
	local val = nil
	for i = 0, 11 do
		if i < 6 then
			val = RED
		else
			val = WHITE
		end
		local index = (i + (whiteStart - 2)) % 12 + 2
		pset(pal, index, val)
	end
end

-- Draws one line of the ball.
local function plotLine(data, x)
	for y = 0, math.pi, 0.008 do
		local val0 = (math.floor((x + math.pi) * 15 + 0.5)) % 12 + 2
		local val1 = ((val0 - 2) + 6) % 12 + 2
		local val = 0
		if math.floor(y * 2.5) % 2 == 0 then
			val = val0
		else
			val = val1
		end
		local x_ = math.sin(y) * math.cos(x) * 30
		local y_ = math.cos(y) * 30
		local s = math.sin(ROTATE_ANGLE)
		local c = math.cos(ROTATE_ANGLE)
		local posX = x_ * c - y_ * s
		local posY = x_ * s + y_ * c
		local idxX = math.floor(posX + 30.5)
		local idxY = math.floor(posY + 30.5)
		local idx = idxX + idxY * data.width + 1
		data.data[idx] = val
	end
end

function setup()
	-- Initialize the canvas.
	local txt = 'Loading...'
	local canvasWidth, canvasHeight = Canvas.main:size()
	local textWidth, textHeight = measure(txt)
	text(txt, (canvasWidth - textWidth) * 0.5, (canvasHeight - textHeight) * 0.5)
	sync()

	sfx = Resources.load('pong.wav', Sfx)
	pal = Resources.load('palette.pal')

	-- Prefab the chequered ball.
	local data = {
		width = 61, height = 61,
		depth = 8,
		ref = 'palette.pal',
		data = {
		}
	}
	for i = 1, data.width * data.height do
		data.data[i] = 0
	end
	for x = 0, math.pi / 2 + 0.025, 0.025 do
		plotLine(data, x)
	end
	for x = math.pi, math.pi / 2 + 0.025, -0.025 do
		plotLine(data, x)
	end
	ball = Resources.load(data, Texture)
end

function update(delta)
	-- Show the background.
	background()

	-- Process bouncing.
	local canvasWidth, canvasHeight = Canvas.main:size()
	parabelX = parabelX + parabelDir * delta
	local ballY = parabelX * parabelX * 1.5
	local maxY = canvasHeight - 83
	if ballY >= maxY then
		ballY = maxY
		parabelX = math.sqrt(ballY / 1.5)
		parabelDir = -parabelDir
	end

	-- Move the ball left and right.
	if ballX <= 0 then
		ballX = 0
		ballDir = -ballDir
		play(sfx)
	elseif ballX >= canvasWidth - 61 then
		ballX = canvasWidth - 61
		ballDir = -ballDir
		play(sfx)
	end
	ballX = ballX + ballDir * delta * 50

	-- Show shadow of the ball.
	clip(23, 0, canvasWidth - 26, canvasHeight - 33)
	circ(ballX + 40, ballY + 37, 30, true, Color.new(20, 20, 20, 110))
	clip()

	-- Cycle palette and show the ball.
	cyclePalette()
	tex(ball, ballX, ballY + 18)
	if ballY >= maxY then
		play(sfx)
	end
end

